                      src/symbolic-regression-demo.hs


  -- Modules included in this executable, other than Main.
  -- other-modules:

  -- Other library packages from which modules are imported.
  build-depends:
                        base >=4.8 && < 5
                        -- haskell platform
                        , mtl ==2.2.*
                        , primitive ==0.6.*
                        , vector ==0.11.*
